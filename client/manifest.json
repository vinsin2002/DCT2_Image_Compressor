{
    "name": "Image Compressor",
    "version": "1.0",
    "description": "An Image compressing extension.",
    "permissions": ["tabs"],
    "background": {
      "scripts": ["background.js"]
    },
    "popup": {
      "html": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ]
  }